# ReSharper properties
resharper_align_linq_query = true
resharper_for_built_in_types = use_var_when_evident
resharper_object_creation_when_type_evident = explicitly_typed
resharper_space_within_single_line_array_initializer_braces = false

# ReSharper inspection severities
resharper_arrange_namespace_body_highlighting = suggestion
resharper_arrange_redundant_parentheses_highlighting = hint
resharper_arrange_this_qualifier_highlighting = hint
resharper_arrange_type_member_modifiers_highlighting = hint
resharper_arrange_type_modifiers_highlighting = hint
resharper_built_in_type_reference_style_for_member_access_highlighting = hint
resharper_built_in_type_reference_style_highlighting = none
resharper_cpp_clang_tidy_clang_diagnostic_float_equal_highlighting = none
resharper_cpp_clang_tidy_clang_diagnostic_shadow_highlighting = suggestion
resharper_cpp_too_wide_scope_highlighting = none
resharper_cpp_too_wide_scope_init_statement_highlighting = none
resharper_inconsistently_synchronized_field_highlighting = hint
resharper_member_can_be_private_global_highlighting = none
resharper_parameter_hides_member_highlighting = hint
resharper_possible_multiple_enumeration_highlighting = hint
resharper_possible_null_reference_exception_highlighting = hint
resharper_redundant_base_qualifier_highlighting = warning
